<% if user_signed_in? %>
  <div class="search-form">
    <%= form_with url: search_users_path, method: :get, local: true do |f| %>
      <%= f.text_field :search %>
      <%= select_tag(:option, options_for_select([["user", "1"],["book","2"]]))%>
      <%= select_tag(:choice, options_for_select([["部分一致", "1" ],["後方一致", "2" ],["前方一致", "3" ],["完全一致", "4" ]])) %>
      <%= f.submit :search %>
    <% end %>
  </div>
<% end %>